{
  "crons": [
    {
      "path": "/api/v1/internal/cron-refresh",
      "schedule": "*/5 * * * *"
    }
  ],
  "buildCommand": "pip install uv && uv pip install -r requirements.txt",
  "devCommand": "uvicorn src.api.main:app --host 0.0.0.0 --port 8000",
  "installCommand": "pip install uv",
  "outputDirectory": ".",
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.11"
    }
  }
}

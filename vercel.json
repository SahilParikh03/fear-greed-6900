{
  "version": 2,
  "crons": [
    {
      "path": "/api/v1/internal/cron-refresh",
      "schedule": "*/5 * * * *"
    }
  ],
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ]
}
